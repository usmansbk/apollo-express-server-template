{
  "name": "auth-service",
  "version": "1.0.0",
  "scripts": {
    "start": "node dist",
    "dev": "nodemon --exec babel-node src",
    "clean": "rimraf dist",
    "transpile": "babel src -d dist --copy-files",
    "build": "yarn clean && yarn transpile",
    "test": "cross-env DOTENV_CONFIG_PATH=.env.test jest -i",
    "createroot": "node dist/scripts/createRoot",
    "createapp": "node dist/scripts/createApp",
    "listapp": "node dist/scripts/listApp",
    "prepare": "husky install",
    "createdb:test": "cross-env DOTENV_CONFIG_PATH=.env.test sequelize db:create --env test",
    "seed": "sequelize-cli db:seed:all",
    "docker:start": "docker compose up",
    "docker:cli": "docker exec -it auth-service sh",
    "docker:build": "docker build -t usmansbk/auth-serivce .",
    "docker:test": "docker compose run --rm api yarn test"
  },
  "description": "Sequelize GraphQL server template",
  "main": "index.js",
  "repository": "git@github.com:usmansbk/apollo-express-server-template.git",
  "author": "Usman Suleiman <usmansbk@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.18.9",
    "@babel/core": "^7.18.9",
    "@babel/node": "^7.18.9",
    "@babel/plugin-transform-runtime": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/register": "^7.18.9",
    "@faker-js/faker": "^7.3.0",
    "babel-jest": "^28.1.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.1",
    "ioredis-mock": "^8.2.2",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "sequelize-cli": "^6.4.1",
    "sync-dotenv": "^2.7.0",
    "weak-napi": "^2.0.2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.131.0",
    "@aws-sdk/client-ses": "^3.131.0",
    "@aws-sdk/credential-provider-node": "^3.131.0",
    "@babel/runtime": "^7.18.9",
    "@graphql-tools/load-files": "^6.6.0",
    "@graphql-tools/merge": "^8.3.0",
    "@graphql-tools/schema": "^8.5.0",
    "@graphql-tools/utils": "^8.8.0",
    "@sentry/node": "^7.7.0",
    "@sentry/tracing": "^7.7.0",
    "analytics-node": "^6.1.0",
    "apollo-datasource": "^3.3.2",
    "apollo-server-core": "^3.10.0",
    "apollo-server-express": "^3.10.0",
    "atob": "^2.1.2",
    "bcrypt": "^5.0.1",
    "btoa": "^1.2.1",
    "bunyan": "^1.8.15",
    "connect-timeout": "^1.9.0",
    "cors": "^2.8.5",
    "dataloader": "^2.1.0",
    "dayjs": "^1.11.3",
    "deepmerge": "^4.2.2",
    "dotenv": "^16.0.1",
    "email-templates": "^10.0.0",
