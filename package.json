{
  "name": "auth-service",
  "version": "1.0.0",
  "scripts": {
    "start": "cross-env APP_NAME=auth-service node -r dotenv/config build",
    "dev": "nodemon --exec babel-node -r dotenv/config src",
    "clean": "rimraf build",
    "transpile": "babel src -d build --copy-files",
    "build": "yarn clean && yarn transpile",
    "test": "cross-env NODE_ENV=test jest -i",
    "createsu": "node -r dotenv/config build/scripts/createSuperUser",
    "createapp": "node -r dotenv/config build/scripts/createApplication",
    "listapp": "node -r dotenv/config build/scripts/listApplications",
    "prepare": "husky install",
    "createdb:test": "docker compose run --rm api npx sequelize db:create --env test",
    "seed": "sequelize-cli db:seed:all",
    "docker:start": "docker compose up",
    "docker:cli": "docker exec -it auth-service sh",
    "docker:test": "docker compose run --rm api yarn test"
  },
  "description": "Sequelize GraphQL server template",
  "main": "index.js",
  "repository": "git@github.com:usmansbk/apollo-express-server-template.git",
  "author": "Usman Suleiman <usmansbk@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/node": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/register": "^7.18.6",
    "@faker-js/faker": "^7.3.0",
    "babel-jest": "^28.1.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.1",
    "ioredis-mock": "^8.2.2",
    "jest": "^28.1.2",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.16",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "sequelize-cli": "^6.4.1",
    "weak-napi": "^2.0.2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.120.0",
    "@aws-sdk/client-ses": "^3.118.1",
    "@aws-sdk/credential-provider-node": "^3.118.1",
    "@babel/runtime": "^7.18.6",
    "@graphql-tools/load-files": "^6.6.0",
    "@graphql-tools/merge": "^8.3.0",
    "@graphql-tools/schema": "^8.5.0",
    "@graphql-tools/utils": "^8.8.0",
    "@sentry/node": "^7.3.1",
    "@sentry/tracing": "^7.3.1",
    "analytics-node": "^6.0.0",
    "apollo-datasource": "^3.3.2",
    "apollo-server-core": "^3.9.0",
    "apollo-server-express": "^3.9.0",
    "atob": "^2.1.2",
    "bcrypt": "^5.0.1",
    "btoa": "^1.2.1",
    "bunyan": "^1.8.15",
    "connect-timeout": "^1.9.0",
    "cors": "^2.8.5",
    "dataloader": "^2.1.0",
    "dayjs": "^1.11.3",
    "deepmerge": "^4.2.2",
    "dotenv": "^16.0.1",
    "email-templates": "^9.0.0",
    "express": "^4.18.1",
    "express-rate-limit": "^6.4.0",
    "google-auth-library": "^8.0.3",
