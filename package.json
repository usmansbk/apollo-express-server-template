    "inquirer": "^8.2.4",
    "ioredis": "^5.1.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "nanoid": "^3.3.4",
    "node-fetch": "2.6.7",
    "nodemailer": "^6.7.5",
    "numeral": "^2.0.6",
    "pem-jwk": "^2.0.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "pug": "^3.0.2",
    "rate-limit-redis": "^3.0.1",
    "retry": "^0.13.1",
    "sequelize": "^6.21.2",
    "twilio": "^3.78.0"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "nodemonConfig": {
    "ext": "js,gql,env"
  }
}
