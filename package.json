    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "nanoid": "^3.3.4",
    "node-fetch": "2.6.7",
    "nodemailer": "^6.7.7",
    "numeral": "^2.0.6",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "pug": "^3.0.2",
    "rate-limit-redis": "^3.0.1",
